{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "rescript-react-intro",
  "reason": {
    "react-jsx": 3
  },
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": ["-bs-super-errors"],
  "refmt": 3,
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "gentypeconfig": {
    "language": "typescript"
  },
  "ppx-flags": [
    "lenses-ppx/ppx"
  ],
  "bs-dependencies": [
    "@rescript/react",
    "bs-css",
    "bs-css-emotion",
    "@rescriptbr/reform",
    "reschema",
    "@rescriptbr/ancestor"
  ]
}
